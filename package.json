{
  "name": "milkman-api",
  "version": "0.1.0",
  "description": "Module-based Milkman backend API",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "dev": "node --watch src/server.js",
    "start": "node src/server.js",
    "migrate": "node node_modules/.bin/knex --knexfile knexfile.js migrate:latest",
    "migrate:rollback": "node node_modules/.bin/knex --knexfile knexfile.js migrate:rollback",
    "lint": "eslint \"src/**/*.js\""
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.0.0"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.8.5",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "firebase-admin": "^12.7.0",
    "get-stream": "^8.0.1",
    "helmet": "^6.2.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.11.3",
    "node-cron": "^3.0.3",
    "objection": "^3.1.4",
    "pdfkit": "^0.15.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-vector-icons": "^10.3.0",
    "zod": "^3.23.8"
  }
}
